<librec-auto>
	<random-seed>19990</random-seed>
	<thread-count>1</thread-count>
	<data>
		<data-dir>../../../data/rerank-data</data-dir>
		<format>UIR</format>
		<data-file format="text">ratings.csv</data-file>
	</data>
	
	
	<features>
		<appender-class>net.librec.data.convertor.appender.ItemFeatureAppender</appender-class>
		<item-feature-file>item-features.csv</item-feature-file>
	</features>
	
	
	<splitter>
		<model count="2">kcv</model>
		<dim>userfixed</dim> 
		
		<ratio>0.8</ratio>
		<save>true</save>
	</splitter>
	
	
	
	<alg>
		<class>biasedmf</class>
		<learn-rate>0.01</learn-rate>
		<learn-rate-max>0.01</learn-rate-max>
		<learn-rate-decay>1.0</learn-rate-decay>
		<bold-driver>false</bold-driver>
		<iterator-max>25</iterator-max>
		<user-reg>0.01</user-reg>
		<item-reg var="true">0.01</item-reg><bias-reg>0.01</bias-reg>
		<num-factors>4</num-factors>
		<similarity type="item">pcc</similarity>
	</alg>
	
	<rerank>
		<script lang="python3" src="system">
			<script-name>far_rerank.py</script-name>
			<param name="max_len">4</param>
			<param var="true" name="lambda">0.3</param>
			<param name="binary">False</param>
		</script>
	</rerank>

	
	<metric>
		<ranking>true</ranking>
		<list-size>50</list-size>
		<class>ndcg,precision,sp</class>
		<protected-feature>new</protected-feature>
	</metric>
<!--This configuration file was automatically generated by librec-auto. Editing may produce unpredictable results and is not recommended.--></librec-auto>
